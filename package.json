{
  "name": "feedback-app-frontend",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Aplicação de Feedback com classificação por cores",
  "private": true,
  "scripts": {
    "start": "serve -s dist",
    "postinstall": "node render-temp-build-fix.js",
    "build": "webpack --mode production",
    "render-build": "node render-build.js",
    "dev": "webpack serve --mode development --open",
    "prebuild": "rimraf dist",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "prepare": "husky install",
    "prepare:ci": "npm run prepare",
    "pre-commit": "lint-staged",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "feedback",
    "frontend",
    "dashboard",
    "javascript",
    "webpack"
  ],
  "author": "",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "chart.js": "^3.7.0",
    "core-js": "^3.19.0",
    "date-fns": "^2.25.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.22.11",
    "@babel/preset-env": "^7.22.11",
    "@babel/preset-react": "^7.22.5",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "core-js": "^3.19.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.2",
    "prettier": "^2.5.0",
    "rimraf": "^6.0.1",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "serve": "^14.2.5",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
